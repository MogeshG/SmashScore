{
  "expo": {
    "name": "SmashScore",
    "slug": "smash-score",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "/assets/images/Smash.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mogesh.smashscore"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mogesh.smashscore"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": ""
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "4ca79074-35ff-4a48-b1cf-273ae68c2cf1"
      }
    },
    "owner": "mogesh"
  }
}
